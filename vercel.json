{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/css/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/js/$1"
    },
    {
      "src": "/demos/(.*)",
      "dest": "/demos/$1"
    },
    {
      "src": "/markmap.svg",
      "dest": "/markmap.svg"
    },
    {
      "src": "/demos.html",
      "dest": "/demos.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
